baseline land[LUC]=
	Tot Baseline Land*BASELINE LUC SHARE[LUC]
	~	ha
	~		|

carrying capacity R[LUC]=
	INI CARRYING CAPACITY[LUC]*baseline land[LUC]-Baseline Stock Units[LUC]
	~	
	~		|

LUC:
	LUC1,LUC2,LUC3,LUC4,LUC5,LUC6,LUC7,LUC8
	~	
	~		|

ANTECEDENT RAINFALL:INTERPOLATE::=
	GET XLS DATA('?basedata', 'L4L' , '1' , 'B2' )
	~	mm
	~		|

Baseline Stock Units[LUC]= INTEG (
	+carrying capacity R[LUC]-Baseline Stock Units[LUC],
		INI STOCK UNITS[LUC])
	~	n
	~		|

carrying capacity I[LUC]=
	SMOOTH((INI CARRYING CAPACITY[LUC]*intervention land[LUC]*productivity)-Intervention Stock Units\
		[LUC], 5)
	~	
	~		|

DIVERSE FACTOR=
	-0.012
	~	Dmnl
	~		|

diverse rt=
	DIVERSE FACTOR*Tot Baseline Land
	~	ha/yr
	~		|

Diversification= INTEG (
	SQRT(diverse rt*diverse rt),
		0)
	~	ha
	~		|

Pole Reduction= INTEG (
	SMOOTH3(EXP(marginal reduction p)-TIME STEP, 5),
		0)
	~	pcnt
	~		|

Tot Treatment Land= INTEG (
	-treatment rate,
		INI TREATMENT LAND)
	~	ha
	~		|

total stock units[None]=
	SUM(Baseline Stock Units[LUC!]) ~~|
total stock units[PoplarPoles]=
	IF THEN ELSE(RETIRE DELAY START=0, SUM(Baseline Stock Units[LUC!]), (SUM(Baseline Stock Units\
		[LUC!])*(1-LAND RETIREMENT SHARE)*RETIRE DELAY START)+SUM(Intervention Stock Units[\
		LUC!])) ~~|
total stock units[ExoticForest]=
	0
	~	n
	~		|

Intervention Stock Units[LUC]= INTEG (
	+carrying capacity I[LUC]-Intervention Stock Units[LUC],
		INI STOCK UNITS[LUC])
	~	n
	~		|

